<div class="header">
    <a href="{{pathPrefix}}">
        {{> icon_logo}}
    </a>
    <div class="header-buttons">
        <form action="http://127.0.0.1:4455/"> <button>Dashboard</button> </form>
    </div>
    <div class="header-buttons">
        <form action="http://127.0.0.1:4455/passport"> <button>Passport</button> </form>
    </div>
    <div class="icon-actions">
        <div class="settings">
            <a href="{{pathPrefix}}settings">
                {{> icon_gear}}
            </a>
        </div>
        <div class="logout">
            <a href="{{logoutUrl}}">
                {{> icon_sign_out}}
            </a>
        </div>
        <div class="fork">
            <a href="https://github.com/ory/kratos-selfservice-ui-node" target="_blank">
                {{> icon_repo_forked}}
                <div>Fork on<br>GitHub</div>
            </a>
        </div>
    </div>
</div>
